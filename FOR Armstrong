#include <stdio.h> 

int main ()
{
    int temp, sum =0,r, n;
    
    printf("Enter a number:\n");
    scanf("%d", &n);
    
    for(temp = n;n != 0;n = n/10 ){
      
      r = n%10;
      sum = sum + r*r*r;
    }
    
  if (temp==sum){
      
      printf("Armstrong number");
  }else {
      printf("not a armstrong number");
  }
      
    return 0;
    
}
        
        
    
    
    
