#include <stdio.h>

int main()
{
    int r, n, i;
    printf("Enter the number to print its table:\n");
    scanf("%d", &n);

    /*
       for (i=1; i<=10; i++){
        r = n*i;
        printf("%dx%d=%d\n", n,i,r);
    }
    */
    i = 1;
    while (i <= 10)
    {
        //r = n*i;
        
        printf("%dx%d=%d\n", n, i, n*i);
        i++;
    }

    return 0;
}
